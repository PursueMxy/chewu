<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.weimu.chewu">

    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".AppData"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:persistent="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.weimu.chewu.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/filepaths" />
        </provider>

        <activity
            android:name=".module.main.MainActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".module.loginx.LoginActivity"
            android:launchMode="singleTask" />
        <activity android:name=".module.login.ForgetPwdTip1Activity" />
        <activity android:name=".module.login.ForgetPwdTip2Activity" />
        <activity android:name=".module.login.ForgetPwdTip3Activity" />
        <activity android:name=".module.wallet.ChooseBankActivity" />
        <activity android:name=".module.wallet.AddCardActivity" />
        <activity android:name=".module.user.UserInfoActivity" />
        <activity android:name=".module.register.RegisterTips1Activity" />
        <activity android:name=".module.register.RegisterTips2Activity" />
        <activity android:name=".module.user.UserHeadActivity" />
        <activity android:name=".module.user.UserSexActivity" />
        <activity android:name=".module.user.UserNicknameActivity" />
        <activity android:name=".module.register.DriverAuthenticationActivity" />
        <activity android:name=".module.register.RegisterTips3Activity" />
        <activity android:name=".module.city.CityListActivity" />
        <activity android:name=".module.order.OrderPageActivity" />
        <activity android:name=".module.wallet.MyWalletActivity" />
        <activity android:name=".module.order_detail_ing.OrderDetailIngActivity" />
        <activity android:name=".module.order_detail_ing.AddRemarkActivity" />
        <activity android:name=".module.order_detail_arrival.OrderDetailArrivalActivity" />
        <activity android:name=".module.order_detail_arrival.AddArrivalPictureActivity" />
        <activity android:name=".module.welcome.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.user.phone.ModifyPhoneCodeActivity" />
        <activity android:name=".module.user.phone.ModifyPhoneNewPhoneActivity" />
        <activity android:name=".module.station.StationActivity" />
        <activity android:name=".module.order_detail_arrival.OrderDetailArrivalV2Activity" />
        <activity android:name=".module.update_report.UpdateReportActivity" />
        <activity android:name=".module.update_report.SignatureActivity" />
        <activity android:name=".module.wallet.bank.money_detail.MoneyDetailActivity" />
        <activity android:name=".module.wallet.bank.WithdrawCashActivity" />
        <activity android:name=".module.contract_service.ContractServiceActivity" />
        <activity android:name=".module.contract_service.ContractServiceDetailActivity" />
        <activity android:name=".module.navi.NaviActivity" />
        <activity android:name=".module.navi.PathPlanActivity" />
        <activity android:name=".module.register.RegisterTips4Activity" />
        <activity android:name=".module.register.RegisterTips5Activity" />
        <activity android:name=".module.station.StationMapActivity" />
        <activity android:name=".module.user.phone.ModifyPhoneNewCodeActivity" />
        <activity android:name=".module.pathtrack.PathTrackActivity" />
        <activity android:name=".module.message.MessageCenterActivity" />

        <activity
            android:name=".module.OnePixelActivity"
            android:theme="@style/OnePixelActivity" />

        <!--推送接收者-->
        <receiver
            android:name=".receiver.JpushReceiver"
            android:enabled="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTRATION" />
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" />
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" />
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" />
                <action android:name="cn.jpush.android.intent.NOTIFICATION_CLICK_ACTION" />
                <action android:name="cn.jpush.android.intent.CONNECTION" />

                <category android:name="com.weimu.chewu" />
            </intent-filter>
        </receiver>
        <!--上传位置的服务-->
        <service
            android:name=".services.UpLoadLocationService"
            android:enabled="true"
            android:exported="true" />
        <!--<service android:name=".services.HHHservice"/>-->
    </application>

</manifest>